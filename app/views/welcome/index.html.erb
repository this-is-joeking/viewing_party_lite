<% if current_user %>
  <h3>Welcome <%= current_user.name %></h3>
  <h3>Existing Users: </h3>
  <div id='existing_users'>
    <% @users.each do |user| %>
      <%= user.email %><br>
    <% end %>
  </div>
<% else %>
<br>
<%= button_to 'Create an account', register_path, method: :get %>
<br>
<%= button_to 'Log In', login_path, method: :get %><br>
<%= button_to 'Browse Movies', discover_index_path, method: :get %>
<% end %>